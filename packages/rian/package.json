{
	"name": "rian",
	"version": "0.2.1",
	"description": "Effective tracing for the edge and origins",
	"keywords": [
		"opentelemetry",
		"trace-context",
		"opentracing",
		"traceparent",
		"measure",
		"monitor",
		"observe",
		"tracing",
		"zipkin",
		"tracer",
		"trace"
	],
	"repository": "maraisr/rian",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./exporter.otel.http": {
			"types": "./exporter.otel.http/index.d.ts",
			"import": "./exporter.otel.http/index.mjs",
			"require": "./exporter.otel.http/index.js"
		},
		"./exporter.zipkin": {
			"types": "./exporter.zipkin/index.d.ts",
			"import": "./exporter.zipkin/index.mjs",
			"require": "./exporter.zipkin/index.js"
		},
		"./utils": {
			"types": "./utils/index.d.ts",
			"import": "./utils/index.mjs",
			"require": "./utils/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./index.js",
	"module": "./index.mjs",
	"types": "index.d.ts",
	"files": [
		"*.mjs",
		"*.js",
		"*.d.ts",
		"exporter.*/*",
		"utils/*"
	],
	"scripts": {
		"build": "bundt --minify"
	},
	"dependencies": {
		"flattie": "^1.1.0",
		"tctx": "^0.0.10"
	},
	"devDependencies": {
		"bundt": "2.0.0-next.5"
	}
}
